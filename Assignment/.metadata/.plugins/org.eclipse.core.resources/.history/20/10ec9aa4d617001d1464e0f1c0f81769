package encapsulation;

public class Bike {

	public static void main(String[] args) {
		BikeInstallments b = new BikeInstallments();
		b.setId(101);
		b.setEngine(220);
		b.setPrice(135000);
		b.setPay(1);
		b.setName("pulsar");
		b.setBrand("bajaj");
		b.setColor("black");
	    System.out.println("Id \t"+"Engine\t\t"+"Price\t"+"Name\t\t"+"Brand\t\t"+"Color\t");
	    System.out.println("----------------------------------------");
        System.out.println(b.getId() + b.getName() + b.getBrand() + b.getEngine() + b.getColor() + b.getDiscountePrice()+ b.getPrice());
	}

}
