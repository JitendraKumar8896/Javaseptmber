package classesobjects;

public class MethodPattern {
       void pattern() {
    	   int r = 5;
    	   for(int i=1;i<=r;i++)
   	    {
   	    	for(int j =1; j<=i;j++)
   	    	{
   	    		System.out.print("*");
   	    	}
   	    	System.out.println();
   	    }
       }
	public static void main(String[] args) {
		

	}

}
