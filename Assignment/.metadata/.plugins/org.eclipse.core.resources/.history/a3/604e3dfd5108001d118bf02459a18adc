package days;

import java.util.Scanner;

public class GivenDigitGrater {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.print("Enter digit:");
		int num = sc.nextInt();
		
		int r,larg=0;
		while(num>0)
		{
		   r = num % 10;
		   if(larg < r)
		   {
			   larg = r;
		   }
			num = num/10;   
		}
      System.out.println("Largest Number Is:"+larg);
	}

}
