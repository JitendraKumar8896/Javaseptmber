package constructor;

public class PersonContainment {

	public static void main(String[] args) {
		
		Address a = new Address("\tKhalilabad","\tUttar Pradesh","\tIndia",272175);
		Address a1 = new Address("Gorakhpur","Uttar Pradesh","India",273001);
		Address a2= new Address("Lucknow","Uttar Pradesh","India",226001);
		Address a3 = new Address("Delhi","Delhi","India",110001);
		Address a4 = new Address("Hydrabad"," telangana","India",500001);
		
		Person p = new Person("Jitendra kumar\t","Male\t",22, a);
		Person p1 = new Person("Harsha","Male",22, a1);
		Person p2= new Person("Shivani","Female",22, a2);
		Person p3 = new Person("Aditya","Male",22, a3);
		Person p4 = new Person("Nitesh","Male",22, a4);
		
		System.out.println("Name \t Gender \t Age \t City \t State \t Country \t Zip Code");
		System.out.println("--------------------------------------------------\n");
		System.out.println(p);
		System.out.println("---------------------------------------------------");
		System.out.println(p1);
		System.out.println("---------------------------------------------------");
		System.out.println(p2);
		System.out.println("---------------------------------------------------");
		System.out.println(p3);
		System.out.println("---------------------------------------------------");
		System.out.println(p4);
		System.out.println("---------------------------------------------------");
	}

}
