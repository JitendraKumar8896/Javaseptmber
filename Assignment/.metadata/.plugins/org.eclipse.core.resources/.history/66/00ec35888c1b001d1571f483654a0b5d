package constructor;

public class StudentDepartment {

	public static void main(String[] args) {
		Department d1 =  new Department(11,"JItendra");
		Department d2 =  new Department(12,"Vijendra");
	    Department d3 =  new Department(13,"Amit");

	    Student s1 = new Student("Computer Science ",110);
	    Student s2 = new Student("Computer Science ",111);
	    Student s3 = new Student("Computer Science ",112);
	    
	    System.out.println();
	    System.out.println(s1);
	    System.out.println();
	    System.out.println(s2);
	    System.out.println();
	    System.out.println(s3);
	    
	}

}
