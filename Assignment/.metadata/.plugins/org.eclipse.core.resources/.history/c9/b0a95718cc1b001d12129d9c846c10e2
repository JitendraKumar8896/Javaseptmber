package constructor;

public class Address {
  String city,state,country;
  int pin;
  Address(String city,String state, String country,int pin)
  {
	  this.city = city;
	  this.state = state;
	  this.country = country;
	//  this.pin = pin;
  }
   public String toString()
   
   {
	   return city+ " "+ state+ " "+country+ " "+pin; 
   }
}
