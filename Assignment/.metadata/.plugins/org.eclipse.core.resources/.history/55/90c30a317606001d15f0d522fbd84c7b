package test;

import java.util.Scanner;

public class EvenWhile {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter Limit:");
		int limit = sc.nextInt();
		int c =1;
		while(c>=limit)
		{
			if(c%2==0)
				System.out.println(c);
			
		c++;	
		}

	}

}
