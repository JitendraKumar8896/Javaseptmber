package constructor;

public class StudentIndividual1 {

	public static void main(String[] args) {
		StudentInd22 s1 = new StudentInd22("111","Jitendra","Third standerd",550);
		StudentInd22 s2 = new StudentInd22("112","Vijendra","Third standerd",555);
		StudentInd22 s3 = new StudentInd22("113","Amit","five standerd",545);
		StudentInd22 s4 = new StudentInd22("114","Akash","fourth standerd",505);
		StudentInd22 s5 = new StudentInd22("115","Sumit","second standerd",550);
	    System.out.println("Roll NUmber \t Name \t Standard \t Total Marks");
	    System.out.println("-----------------------------------------------------");
	    System.out.println(s1.getRollNumber()+" "+s1.getName()+ " "+ s1.getStandard()+" "+s1.getTotalMarks());
	    System.out.println("-----------------------------------------------------");
	    System.out.println(s2.getRollNumber()+" "+s2.getName()+ " "+ s2.getStandard()+" "+s2.getTotalMarks());
	    System.out.println("-----------------------------------------------------");
	    System.out.println(s3.getRollNumber()+" "+s3.getName()+ " "+ s3.getStandard()+" "+s3.getTotalMarks());
	    System.out.println("-----------------------------------------------------");
	    System.out.println(s4.getRollNumber()+" "+s4.getName()+ " "+ s4.getStandard()+" "+s4.getTotalMarks());
	    System.out.println("-----------------------------------------------------");
	    System.out.println(s5.getRollNumber()+" "+s5.getName()+ " "+ s5.getStandard()+" "+s5.getTotalMarks());
	}

}
